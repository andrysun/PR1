public class Pactic1 {
    public Class1() {
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int sum = 0;
        int sr = false;
        int[] mas = new int[10];

        for(int i = 0; i < 10; ++i) {
            mas[i] = sc.nextInt();
            sum += mas[i];
        }

        int sr = sum / 10;
        System.out.println(sum + "\n" + sr);
    }
}
